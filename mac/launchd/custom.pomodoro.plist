<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
    <key>Label</key>
    <string>custom.pomodoro</string>

    <key>RunAtLoad</key>
    <true/>

    <key>ProgramArguments</key>
    <array>
      <string>/usr/bin/osascript</string>
      <string>-e</string>
      <string>display dialog "Take a break!!!" with title "... Go stretch ..."</string>
    </array>

    <key>Debug</key>
    <false/>

    <key>StartInterval</key>
    <integer>1800</integer>

    <key>StandardErrorPath</key>
    <string>/tmp/custom_pomodoro_err</string>

    <key>StandardOutPath</key>
    <string>/tmp/custom_pomodo_stdout</string>
</dict>
</plist>
